{
   "extends": ["eslint-config-airbnb", "prettier"],
   "settings": {
      "ecmascript": 6
   },
   "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "destructuring": true,
      "classes": true,
      "forOf": true,
      "blockBindings": true,
      "arrowFunctions": true
   },
   "env": {
      "browser": true
   }
}
